{
	"manifest_version": 2,
	"name": "Crop Element",
	"description": "Adds a 'Crop Element' menu item to right click menu",
	"version": "0.1",
	"permissions": [
		"contextMenus"
	],
	"background": {
		"scripts": ["back.js"]
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["crop.js"]
		}
	]
}
